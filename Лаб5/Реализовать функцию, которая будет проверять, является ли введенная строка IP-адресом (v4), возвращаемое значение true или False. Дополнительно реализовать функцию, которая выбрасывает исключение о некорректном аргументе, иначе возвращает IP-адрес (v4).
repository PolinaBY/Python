import re
def is_ipv4(ip):
    pattern = r"^(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$"
    return bool(re.match(pattern, ip))

def iden_ipv4(ip):
    if not is_ipv4(ip):
        return "Некорректный IP-адрес IPv4"
    return ip

ip1 = "192.168.1.1"
ip2 = "256.1.1.1"

print(is_ipv4(ip1))
print(is_ipv4(ip2))
print(iden_ipv4(ip1))
print(iden_ipv4(ip2))
